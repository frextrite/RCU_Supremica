digraph state_automaton {
	center = true;
	// size = "700,800";
	nodesep = 0.5;
	"__init" [shape=plaintext, style=invis, label=""];
	node [shape=ellipse, fontsize=10, color=green3];
	edge [fontsize=10];

	"__init" -> "kernel";
	"kernel" -> "idle" [label="rcu_idle_enter"];
	"idle" -> "kernel" [label="rcu_idle_exit"];

	subgraph init {
		rank = min;
		"__init";
		"kernel";
	}
}
